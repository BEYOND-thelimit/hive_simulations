    <collision>
      <origin xyz="0.12 0.0 -0.395" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find hive_gazebo)/meshes/robots/lidar_link_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="robot3_imu_link"/>

  <joint name="robot3_imu_joint" type="fixed">
    <origin xyz="-0.12 0 0.5" rpy="0 0 0"/>
    <parent link="robot3_base_link"/>
    <child link="robot3_imu_link"/>
  </joint>

  <joint name="robot3_left_wheel_joint" type="continuous">
    <origin xyz="-1e-06 0.2734 0.0" rpy="0 0 0"/>
    <parent link="robot3_base_link"/>
    <child link="robot3_left_wheel"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>

  <joint name="robot3_right_wheel_joint" type="continuous">
    <origin xyz="-1e-06 -0.2734 0.0" rpy="0 0 0"/>
    <parent link="robot3_base_link"/>
    <child link="robot3_right_wheel"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>

  <joint name="robot3_right_caster_joint" type="fixed">
    <origin xyz="-0.31 -0.19 0.043" rpy="0 0 0"/>
    <parent link="robot3_base_link"/>
    <child link="robot3_right_caster"/>
    <axis xyz="0.0 0.0 -1.0"/>
  </joint>

  <joint name="robot3_left_caster_joint" type="fixed">
    <origin xyz="-0.31 0.19 0.043" rpy="0 0 0"/>
    <parent link="robot3_base_link"/>
    <child link="robot3_left_caster"/>
    <axis xyz="0.0 0.0 -1.0"/>
  </joint>

  <joint name="robot3_lidar_joint" type="fixed">
    <origin xyz="-0.12 0.0 0.405" rpy="0 0 0"/>
    <parent link="robot3_base_link"/>
    <child link="robot3_lidar_link"/>
    <axis xyz="-0.0 -0.0 1.0"/>
  </joint>

</robot>
