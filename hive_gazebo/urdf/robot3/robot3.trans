<?xml version="1.0" ?>
<robot name="robot3" xmlns:xacro="http://www.ros.org/wiki/xacro" >

<transmission name="robot3_left_wheel_joint_tran">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="robot3_left_wheel_joint">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
  </joint>
  <actuator name="robot3_left_wheel_joint_actr">
